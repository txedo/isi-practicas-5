order(&amp;num1, &amp;num3);
order(&amp;num2, &amp;num3);

printf(&quot;Los numeros en orden ascendente son: %.2f %.2f %.2f&quot;,&amp;num1, &amp;num2, &amp;num3);

return 0;
}



